{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "plugin:cypress/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "space-before-function-paren": "off"
  }
}
